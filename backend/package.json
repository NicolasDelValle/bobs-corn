{
  "name": "bobs-corn-backend",
  "version": "1.0.0",
  "description": "Bob's Corn - Full-stack starter with Express, TypeScript, Prisma, React, and Docker",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --ignore 'node_modules/**' --ignore 'dist/**' src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "NODE_ENV=development npm run dev",
    "start:prod": "NODE_ENV=production npm run start",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "tsc --noEmit"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "node-cron": "^4.2.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^4.0.8",
    "nodemon": "^3.1.11",
    "prisma": "^6.19.0",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}
